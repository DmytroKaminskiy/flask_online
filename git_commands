
0. git init (???)

1. git status
2. git add {filename or directory}
3. git commit -m "message"
4. git push -u origin main
1-4

5. git diff [filename]
6. git log
7. git pull origin {branch_name}

8. git branch {name} - create branch
9. git checkout {branch_name} - change branch
# config
$ git config --global user.name "John Doe"
$ git config --global user.email johndoe@example.com

# git status should be clean
1. git checkout main
2. git pull origin main
3. git branch homework4
4. git checkout homework4
# after homework is done
5. git add --all
6. git commit -m "hw4"
7. git push origin homework4

# if comment
1. fix comments
2. git add --all
3. git commit -m "hw4"
4. git push origin homework4
